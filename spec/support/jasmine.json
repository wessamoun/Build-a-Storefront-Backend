{
  "spec_dir": "build",
  "spec_files": [
    "models/tests/*[sS]pec.js",
    "handlers/tests/*[sS]pec.js"
  ],
  "helpers": [
    "**/helpers/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": true
}
